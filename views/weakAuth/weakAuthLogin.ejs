<%- include('../partials/header') -%>

<main class="container signatureDiv authDiv">
    <h2 id="authIntro">Weak Authentication Login</h2>
    <form action="/weakAuth/login" method="POST">

        <label for="username">Username</label>
        <input id="username" class="form-control">

        <label for="password">Password</label>
        <input id="password" class="form-control">

        <button type="submit" class="btn signatureBtn">Login</button>
    </form>
    <div>
        <a onclick="window.location='/weakAuth/signup'">Don't Have An Account? Signup</a>
    </div>
</main>
    
<%- include('../partials/header') -%>